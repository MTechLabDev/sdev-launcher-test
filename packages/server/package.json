{
    "name": "@aurora-launcher/server",
    "version": "0.0.5-dev.99",
    "description": "LauncherServer for AuroraLauncher",
    "keywords": [
        "minecraft",
        "launcher"
    ],
    "homepage": "https://aurora-launcher.ru",
    "bugs": {
        "url": "https://github.com/AuroraTeam/AuroraLauncher/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AuroraTeam/AuroraLauncher.git"
    },
    "license": "MIT",
    "author": "AuroraTeam",
    "contributors": [
        "JoCat (https://github.com/JoCat)",
        "LoomeL (https://github.com/LoomeL)",
        "FIXxp (https://github.com/FIXxp)",
        "IsTopNick (https://github.com/IsTopNick)"
    ],
    "main": "dist/LauncherServer.js",
    "scripts": {
        "dev": "tsup --watch --onSuccess \"node .\"",
        "build": "tsup",
        "build:bin": "pkg dist/LauncherServer.js -t node20-linux-x64,node20-macos-x64,node20-win-x64 --out-path dist --compress GZip",
        "clean": "rimraf dist",
        "prettier": "prettier --write src",
        "typecheck": "tsc --noEmit",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "start": "node ."
    },
    "devDependencies": {
        "@eslint/js": "^9.32.0",
        "@types/cli-progress": "^3.11.6",
        "@types/hjson": "^2.4.6",
        "@types/node": "^22.10.7",
        "@types/semver": "^7.5.8",
        "@types/uuid": "^10.0.0",
        "@types/ws": "^8.5.13",
        "@yao-pkg/pkg": "^6.2.0",
        "concurrently": "^9.1.2",
        "eslint": "^9.32.0",
        "globals": "^15.14.0",
        "import-sort-style-module": "^6.0.0",
        "prettier": "^3.4.2",
        "prettier-plugin-import-sort": "0.0.7",
        "rimraf": "^6.0.1",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.38.0"
    },
    "dependencies": {
        "@aurora-launcher/core": "workspace:^",
        "@fastify/multipart": "^9.2.1",
        "@fastify/static": "^8.2.0",
        "@freshgum/typedi": "^0.7.2",
        "@tsconfig/node22": "^22.0.2",
        "@tsconfig/strictest": "^2.0.5",
        "chalk": "^5.4.1",
        "chokidar": "^4.0.3",
        "class-transformer": "^0.5.1",
        "cli-progress": "^3.12.0",
        "commander": "^13.0.0",
        "es-toolkit": "^1.39.10",
        "fastify": "^5.3.3",
        "hash-wasm": "^4.12.0",
        "hjson": "^3.2.2",
        "semver": "^7.6.3",
        "source-map-support": "^0.5.21",
        "strip-ansi": "^7.1.0",
        "uuid": "^11.0.5"
    },
    "importSort": {
        ".js, .mjs, .ts": {
            "style": "module",
            "parser": "typescript"
        }
    }
}
